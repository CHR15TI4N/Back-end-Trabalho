// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model receitas {
  id Int @id @default(autoincrement())
  name String
  descricao String
  tempPreparo String
}

model users {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
}

